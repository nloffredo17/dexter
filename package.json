{
  "name": "dexter",
  "version": "2026.2.6",
  "description": "Dexter - AI agent for deep financial research.",
  "type": "module",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "typecheck": "turbo run typecheck",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "audit": "bun audit",
    "cli": "bun run apps/cli/src/index.tsx",
    "postinstall": "playwright install chromium"
  },
  "devDependencies": {
    "turbo": "^2.3.0"
  },
  "dependencies": {
    "playwright": "^1.52.0"
  }
}
